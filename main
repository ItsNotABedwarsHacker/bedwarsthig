-- Declare a function to handle the long jump
function longJump()
   -- Set a timer to track the elapsed time
   local timer = 0

   -- Update the timer every frame
   while timer < 3 do
      timer = timer + 1/60
      player:setVelocity(0, 10)
      yield()
   end
end

-- Bind the long jump function to a key press event
input:bind("r", longJump)
	end,

